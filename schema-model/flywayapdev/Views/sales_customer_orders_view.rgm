view "flywayapdev"."sales_customer_orders_view" {
  text = select `c`.`CustomerID` AS `CustomerID`,`c`.`FirstName` AS `FirstName`,`c`.`LastName` AS `LastName`,`o`.`OrderID` AS `OrderID`,`o`.`OrderDate` AS `OrderDate`,`o`.`Status` AS `Status`,`o`.`TotalAmount` AS `TotalAmount` from (`flywayapdev`.`customers_customer` `c` join `flywayapdev`.`sales_orders` `o` on((`c`.`CustomerID` = `o`.`CustomerID`)))
}

